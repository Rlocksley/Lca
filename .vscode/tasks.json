{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build lca_example1 (Debug)",
      "type": "shell",
      "command": "cmake",
      "args": [
        "--build",
        "${workspaceFolder}/build/examples/example1",
        "--config",
        "Debug",
        "--target",
        "lca_example1"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$msCompile"
      ]
    },
    {
      "label": "build lca_example1 (Release)",
      "type": "shell",
      "command": "cmake",
      "args": [
        "--build",
        "${workspaceFolder}/build/examples/example1",
        "--config",
        "Release",
        "--target",
        "lca_example1"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$msCompile"
      ]
    }
  ]
}
